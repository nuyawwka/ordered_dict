class MyOrderedDict(object):
    def __init__(self):
        self.dict = {}
        self.keys = []

    def get(self, k):
        return self.dict.get(k, [None])[0]

    def add(self, k, v):
        if k in self.dict:
            self.keys.pop(self.dict[k][1])
            
        self.dict[k] = (v, len(self.keys))
        self.keys.append(k)
                    
d = MyOrderedDict()
d.add('a', 1)
d.add('b', 2)
d.add('c', 3)
d.add('a', 4)

for i in d.keys:
    print(i, d.get(i))

 
